        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Check where the user is moving the cursor in the menu.

// If we're appropriately in the menu and the inventory was selected
if (global.gameMenu_ON &amp;&amp; global.inventoryGUI_ON &amp;&amp; !global.textbox_ON &amp;&amp; global.itemMenu_ON)
{
    var move = 0;

// move will dec by 1 if you're pressing the left array key or A
   move -= max(keyboard_check_pressed(vk_left), keyboard_check_pressed(ord("A")), 0);

// move will inc by 1 if you're pressing right arrow key or D
   move += max(keyboard_check_pressed(vk_right), keyboard_check_pressed(ord("D")), 0);

// move = 0 means that right now, we don't want to move.

// Otherwise, change the cursor position, in a circular direction.
   if (move != 0) {
      cursorPos += move;
      if (cursorPos &lt; 0) {
         cursorPos = 1;
      }
      if (cursorPos &gt;= 2) {
            cursorPos = 0;
      }
   }

    // Select an item and do the thing.
    var select;
    select = max(keyboard_check_released(vk_enter), keyboard_check_released(ord("Z")), 0);

    if (select == 1) { 
       scr_inventoryOptions(selectedItem);
    }

}

else { cursorPos = 0; }
</string>
