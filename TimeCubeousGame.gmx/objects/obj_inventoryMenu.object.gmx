            <string>// Runs the menu module create event first.
event_inherited();

// Change this if you want a different orientation. 0 is default.
orientation = 0;
scr_setKeysByOrientation(orientation);

// Set script to run upon selecting from this menu.
scriptToRun = scr_itemSelected;

// Menu isn't array here, so set to 0 for ds_list.
menu_IsArray = 0;

// List will stack items that you have more than one of.
inventoryGUI = ds_list_create();

for (var i = 0; i &lt; ds_list_size(global.inventory); i++) {
    var item = global.inventory[| i];
    var iName = global.inventory[| i].name;
    var ind = ds_list_find_index(inventoryGUI, iName);
    if (ind &gt; -1) {
       count[ind]++;
    }
    else {
         itemList[i] = item; // Store the item ID so we can access it for the remove function
         ds_list_add(inventoryGUI, iName);
         count[i] = 1;
    }
}

menu = inventoryGUI;

// Turn the menu on once you build the menu.
menu_ON = 1;
</string>
